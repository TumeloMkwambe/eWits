{
  "name": "hello",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.7.7",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-big-calendar": "^1.13.4",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.26.2",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.12",
    "vercel": "^37.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "CI=false react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false --coverageReporters=\"text\" \"lcov\"",
    "codecov": "codecov",
    "test:watch": "react-scripts test --coverage --transformIgnorePatterns \"node_modules/(?!@codemirror)/\"",
    "test:ci": "react-scripts test --coverage --watchAll=false --ci --transformIgnorePatterns \"node_modules/(?!@codemirror)/\"",
    "test:untested": "jest --testPathPattern='(displayEvents|eventDetails|eventCalendar).test.js'",
    "test:single": "jest --testPathPattern",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "codecov": "^3.8.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "react-test-renderer": "^18.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/setupTests.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
